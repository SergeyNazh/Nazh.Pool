<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>disable</Nullable>
		<Title>Nazh.Pool</Title>
		<Authors>Sergey Nazh</Authors>
		<Version>8.0.1</Version>
		<Description>Generic Reusable Object Pool .NET 8</Description>
		<PackageProjectUrl>https://github.com/SergeyNazh/Nazh.Pool</PackageProjectUrl>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<RepositoryUrl>https://github.com/SergeyNazh/Nazh.Pool</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>Nazh;Nazh.Pool;ObjectPool;Pool;Reuse;</PackageTags>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	</PropertyGroup>

	<ItemGroup>
	  <None Include="..\LICENSE">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	  <None Include="..\README.md">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	</ItemGroup>

</Project>
